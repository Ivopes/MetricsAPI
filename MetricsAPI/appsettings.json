{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "MetricsUpdate": {
    "UpdateFrequency": "Hour",
    "DayOfWeek": "Saturday",
    "Hour": 5,
    "MockUpdate": {
      "Enabled": false,
      "Minute": 40
    }
  }
}
